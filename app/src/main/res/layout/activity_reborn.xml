        android:background="@drawable/rebornida1"
        android:src="@drawable/shadowreborn"
        android:scaleType="fitXY"
        app:layout_constraintBottom_toTopOf="@+id/rebornUpgrade1"
        app:layout_constraintStart_toEndOf="@+id/rebornUpgrade1"
        tools:ignore="SpeakableTextPresentCheck" />
    <ImageButton
